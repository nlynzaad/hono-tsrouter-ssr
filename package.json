    "react-dom": "^19.1.0",
    "react-sse-hooks": "^1.0.6"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.19.1",
    "@types/bun": "latest",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "@tanstack/router-plugin": "^1.120.13",
    "@tanstack/react-router-devtools": "1.120.13",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
